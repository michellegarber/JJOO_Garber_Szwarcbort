<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - NombreProyecto</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    
</head>

<body>
    <header>
        <!--Principio nav-->
        <nav>
            <ul>
                <li><a href='@Url.Action("Index")'><img src="/img/logo.png" alt=""></a></li>
                <li><a href='@Url.Action("Historia")'>Historia</a></li>
                <li><a href='@Url.Action("Deportes")'>Deportes</a></li>
                <li><a href='@Url.Action("Paises")'>Paises</a></li>
                <li><a href='@Url.Action("AgregarDeportista")'>Agregar Deportista</a></li>
                <li><a href='@Url.Action("Creditos")'>Créditos</a></li>
                
            </ul>
        </nav>
        <!--fin nav-->
        
    </header>

    <main role="main">
    
        @RenderBody()
    </main>

    <footer>
        @DateTime.Today.ToShortDateString() Martina Szwarcbort y Michelle Garber
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
